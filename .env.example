# Environment variables for Spotify API
# Copy this file to .env and fill in your credentials

SPOTIFY_CLIENT_ID=client_id_from_developer_dashboard
SPOTIFY_CLIENT_SECRET=client_secret_from_developer_dashboard

# For delete/private playlists: paste an OAuth user token with scopes
# playlist-read-private playlist-modify-private playlist-modify-public

SPOTIFY_USER_TOKEN=your_user_token_here
